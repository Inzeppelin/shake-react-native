{
  "name": "example",
  "version": "2.3.1",
  "private": true,
  "scripts": {
    "win:android:uat": "set ANDROID_DEPENDENCY=com.shakebugs.android:shake-uat&& react-native run-android",
    "win:android:stg": "set ANDROID_DEPENDENCY=com.shakebugs.android:shake-staging&& react-native run-android",
    "win:android:prd": "set ANDROID_DEPENDENCY=com.shakebugs.android:shake&& react-native run-android",
    "win:ios:uat": "powershell write-host -fore White -back DarkRed Running iOS app is not supported on Windows OS.",
    "win:ios:stg": "powershell write-host -fore White -back DarkRed Running iOS app is not supported on Windows OS.",
    "win:ios:prd": "powershell write-host -fore White -back DarkRed Running iOS app is not supported on Windows OS.",
    "mac:android:uat": "export ANDROID_DEPENDENCY=com.shakebugs.android:shake-uat && react-native run-android",
    "mac:android:stg": "export ANDROID_DEPENDENCY=com.shakebugs.android:shake-staging && react-native run-android",
    "mac:android:prd": "export ANDROID_DEPENDENCY=com.shakebugs.android:shake && react-native run-android",
    "mac:ios:uat": "export IOS_DEPENDENCY=Shake-UAT && npm run mac:ios:clean && react-native run-ios",
    "mac:ios:stg": "export IOS_DEPENDENCY=Shake-Staging && npm run mac:ios:clean && react-native run-ios",
    "mac:ios:prd": "export IOS_DEPENDENCY=Shake && npm run mac:ios:clean && react-native run-ios",
    "mac:ios:clean": "rm -rf ios/Pods ios/Podfile.lock; cd ios && pod install && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.3.0",
    "@react-native-community/masked-view": "^0.1.10",
